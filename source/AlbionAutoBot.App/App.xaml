<Application 
    x:Class="AlbionAutoBot.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:AlbionAutoBot.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/View/Style/Color.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/View/Style/Brush.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/View/Style/Theme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
